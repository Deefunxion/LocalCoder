# Multi-Agent RAG System Configuration
# IMPORTANT: Copy this file to .env and add your own API keys
# NEVER commit .env to git - it contains sensitive credentials!

# OpenRouter API (for faster, more reliable LLMs)
# Get your API key from: https://openrouter.ai/keys
# KEEP THIS SECRET - DO NOT SHARE OR COMMIT TO GIT
OPENROUTER_API_KEY=sk-or-v1-YOUR_API_KEY_HERE

# Agent-specific models (FREE models only)
# Change these to switch between models
ORCHESTRATOR_MODEL=z-ai/glm-4.5-air:free
GRAPH_ANALYST_MODEL=openai/gpt-oss-120b
SYNTHESIZER_MODEL=z-ai/glm-4.5-air:free
FALLBACK_MODEL=z-ai/glm-4.5-air:free

# Available models: 
# - z-ai/glm-4.5-air:free (GLM 4.5 Air - Very fast)
# - openai/gpt-oss-120b (GPT OSS 120B - High quality)
# - deepseek/deepseek-chat-v3.1:free (DeepSeek - Good coding)
# - google/gemini-2.0-flash-exp-1219 (Gemini 2.0 - Latest)

# Cache directories (D: drive to avoid C: drive space issues)
ACADEMICON_CACHE__BASE_DIR=D:/AI-Models

# Database
ACADEMICON_DATABASE__DB_PATH=./academicon_chroma_db
ACADEMICON_DATABASE__COLLECTION_NAME=academicon_code

# Models
ACADEMICON_MODELS__OLLAMA_BASE_URL=http://localhost:11434
ACADEMICON_MODELS__LLM_MODEL=qwen2.5-coder:14b
ACADEMICON_MODELS__LLM_QUANTIZATION=q5_K_M
ACADEMICON_MODELS__EMBEDDING_MODEL=nomic-ai/nomic-embed-text-v1.5

# Model temperatures
ACADEMICON_MODELS__ORCHESTRATOR_TEMPERATURE=0.1
ACADEMICON_MODELS__SYNTHESIZER_TEMPERATURE=0.5

# Retrieval
ACADEMICON_RETRIEVAL__TOP_K=3
ACADEMICON_RETRIEVAL__CHUNK_SIZE=2048
ACADEMICON_RETRIEVAL__CHUNK_OVERLAP=256

# Enable hybrid search and reranking
ACADEMICON_RETRIEVAL__ENABLE_HYBRID_SEARCH=false
ACADEMICON_RETRIEVAL__ENABLE_RERANKING=false

# Indexing
ACADEMICON_INDEXING__TARGET_CODEBASE_PATH=//wsl$/Ubuntu/home/deeznutz/projects/Academicon-Rebuild

# GPU Configuration
ACADEMICON_GPU__USE_ONNX_RUNTIME=true
ACADEMICON_GPU__MAX_VRAM_USAGE_GB=14.0
ACADEMICON_GPU__CUDA_VISIBLE_DEVICES=0

# Query Cache
ACADEMICON_QUERY_CACHE__ENABLE_CACHE=true
ACADEMICON_QUERY_CACHE__CACHE_SIZE=100

# UI
ACADEMICON_UI__WEB_UI_PORT=7860
ACADEMICON_UI__ENABLE_STREAMING=true
ACADEMICON_UI__ENABLE_CONVERSATION_CONTEXT=true

# Monitoring
ACADEMICON_MONITORING__LOG_LEVEL=INFO
ACADEMICON_MONITORING__ENABLE_METRICS=true

# Feature Flags
ACADEMICON_ENABLE_GRAPH_ANALYST=false
ACADEMICON_ENABLE_QUERY_CLASSIFICATION=false
